<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seguimienot13.control.changePassController">
   <children>
      <Label layoutX="195.0" layoutY="42.0" text="CAJA FUERTE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="215.0" layoutY="95.0" text="Digite la combinacion actual">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="415.0" layoutY="325.0" mnemonicParsing="false" onAction="#closeBox" prefHeight="39.0" prefWidth="140.0" style="-fx-background-color: blue;" text="CERRAR CAJA" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Label layoutX="208.0" layoutY="200.0" text="Digite la NUEVA combinacion">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="changePassBT" layoutX="68.0" layoutY="325.0" mnemonicParsing="false" onAction="#changePass" prefHeight="39.0" prefWidth="163.0" style="-fx-background-color: blue;" text="CAMBIAR CLAVE" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <HBox fx:id="actualKeyBoxes" layoutX="134.0" layoutY="124.0" prefHeight="63.0" prefWidth="338.0">
         <children>
            <TextField fx:id="oldnum1" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="oldnum2" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="oldnum3" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="oldnum4" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="oldnum5" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="oldnum6" onKeyTyped="#onKeyOldPass" prefHeight="53.0" prefWidth="54.0" />
         </children>
      </HBox>
      <HBox fx:id="newKeyBoxes" layoutX="131.0" layoutY="237.0" prefHeight="63.0" prefWidth="338.0">
         <children>
            <TextField fx:id="newnum1" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="newnum2" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="newnum3" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="newnum4" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="newnum5" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
            <TextField fx:id="newnum6" onKeyTyped="#onKey" prefHeight="53.0" prefWidth="54.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
